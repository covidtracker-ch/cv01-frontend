<%= partial "partials/header" %>
<%= partial "partials/homelink" %>

<div class="container container--wide">
  <section class="map map--detail">
    <h3>Mappa dettagliata</h3>
    <p>Risposte totali: <b><span id="totalsubs"><%= t(:loading) %></span></b></p>
    <div class="map-container">
      <iframe src="https://experience.arcgis.com/experience/543146be68424528856d57262cc2b02d/" width="600" height="450" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>
    </div>
  </section>
</div>

<div class="container">
  <section class="map">
    <h2>Mappa di calore</h2>
    <h3>Frazione di potenziali casi clinici</h3>
    <p>Frazione di casi classificati positivamente (secondo la nostra definizione di caso) divisa per il numero totale di invii, per codice postale.</p>
    <div class="map-container">
      <iframe src="https://script.google.com/a/macros/astina.ch/s/AKfycbyJ6R3G59HfS5YzkQvmY9xSbf-PsmZbAHDEA6ZwYAO_MkuCRo6v/exec?type=fraction " width="600" height="450" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>
    </div>
  </section>
</div>

<script>
onDOMReady(function() {
  updateTotalSubs('totalsubs');
});
</script>
